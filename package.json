{
  "name": "create-webnami-blog",
  "version": "1.0.0",
  "description": "An opinionated Eleventy-based blogging website generator optimized for SEO, speed, and performance",
  "type": "module",
  "main": "bin/cli.js",
  "bin": {
    "create-webnami-blog": "./bin/cli.js"
  },
  "scripts": {
    "build": "vite build && eleventy",
    "dev": "vite build && eleventy --serve --watch"
  },
  "files": [
    "bin/",
    "config.js",
    "custom.css",
    "images/",
    "pages/",
    "posts/",
    "src/",
    ".eleventy.js",
    ".eleventyignore",
    "vite.config.js",
    "package.json",
    "package-lock.json",
    ".gitignore"
  ],
  "keywords": [
    "eleventy",
    "11ty",
    "blog",
    "blogging-platform",
    "blog-starter",
    "jamstack",
    "static-site-generator",
    "static-blog",
    "tailwindcss",
    "boilerplate",
    "starter-template",
    "markdown",
    "seo"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://webnami.in",
  "bugs": {
    "url": "https://github.com/webnami-dev/webnami/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webnami-dev/webnami.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "cheerio": "^1.1.2",
    "prismjs": "^1.30.0",
    "tailwindcss": "^4.1.11",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@alpinejs/intersect": "^3.14.9",
    "alpinejs": "^3.14.9",
    "fs-extra": "^11.3.0",
    "html-minifier-terser": "^7.2.0",
    "lucide": "^0.513.0"
  }
}
